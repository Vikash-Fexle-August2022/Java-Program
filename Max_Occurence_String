public class Main
{
	public static void main(String[] args) {
	String s="vikassSshh s";
	s=s.toLowerCase();
	int a[]=new int[127];
	int max=0;
	char b=' ';
	for(int i=0;i<s.length();i++)
	{
	    a[s.charAt(i)]=a[s.charAt(i)]+1;
	    
	}
	for(int i=0;i<s.length();i++)
	{
	    if(max<a[s.charAt(i)])
	    {
	        max=a[s.charAt(i)];
	        b=s.charAt(i);
	    }
	}
	System.out.println( b + " "+max);
	
	}
}

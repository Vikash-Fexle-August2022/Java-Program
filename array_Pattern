public class Main
{
	public static void main(String[] args) {
	int a[]={1,12,18,2,5};
	int max=0;
	for(int i=0;i<a.length;i++)
	{
	    if(max<a[i])
	    {
	        max=a[i];
	    }
	}
	for(int i=0;i<max;i++)
	{
	    for(int j=0;j<a.length;j++)
	    {
	        if(a[j]==0)
	        {
	            System.out.print(" ");
	        }
	        else{
	            System.out.print("*");
	            a[j]=a[j]-1;
	        }}
	        System.out.println();
	    
	}
	}
}

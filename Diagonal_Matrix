public class Main
{                           
	public static void main(String[] args) {
	
	int a[][]={{1,2,3},{1,2,3},{1,2,3}};
	int n=3;
    for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
 
                // Condition for principal diagonal
                if (i == j) {
                    System.out.print(a[i][j] + " ");
                }
            }
        }
        System.out.println("");
	}
}

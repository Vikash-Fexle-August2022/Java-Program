public class Main
{
	public static void main(String[] args) {
	String s="vikash";
    String temp=" ";
    
    String rev=" ";
    
    for(int i=0;i<s.length();i++)
    {
        if(i==s.length()-1)
        {
        temp=temp+String.valueOf(s.charAt(i)).toUpperCase();
        }
        else
        {
      temp=temp+s.charAt(i);
        }
    }


    for(int i=temp.length()-1;i>=0;i--)
    {
      rev+=temp.charAt(i);
    }
    System.out.print(rev);
	}
}

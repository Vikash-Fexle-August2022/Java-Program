public class Main
{
	public static void main(String[] args) {
	        String s="Viv";
	        s=s.toLowerCase();
	       int size=s.length();
	       String rev="";
	       for(int i=size-1;i>=0;i--)
	       {
	          rev+=s.charAt(i);
	       }
	       System.out.print(rev);
	       if(s.equals(rev))
	       {
	           System.out.println("y");
	       }
	       else{
	           System.out.println("n");
	       }
	}
}

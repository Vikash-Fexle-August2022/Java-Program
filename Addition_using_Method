import java.util.Scanner;
public class Main
{
    public static int add(int a,int b)
    {
       // System.out.print(sum);
       int sum=a+b;
        return sum;
    }
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	int b=sc.nextInt();
   int sum=add(a,b);
    System.out.print(sum);
	}
}

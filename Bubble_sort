public class Main
{
	public static void main(String[] args) {
	int a[]={3,51,32,1,53,44};
    
	for(int i=0;i<a.length;i++)
	{
	    for(int j=i+1;j<a.length;j++)
	    {
	        if(a[i]>a[j])
	        {
	            int temp=a[i];
	            a[i]=a[j];
	            a[j]=temp;
	        }
	    }
	}
